<h1>Pirate</h1>
<div class="pirate">
<h2>Pirate Name: <%= @pirate.name%></h2>
<h2>Pirate Height: <%=@pirate.height %></h2>
<h2>Pirate Weight: <%=@pirate.weight %></h2>
</div>

<h2>Ships</h2>
<% @ships.each do |ship|%>
<div class="ship">
<p>Ship Name: <%= ship.name %> </p>
<p>Ship Type: <%= ship.type %> </p>
<p>Ship Booty: <%= ship.booty %> </p>
</div>
<% end%>



